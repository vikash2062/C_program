
#include <stdio.h>

int main() {
   int n,binary;
   printf("Enter the decimal to binary :");
   scanf("%d",&n);
   int rem[40];
   int i=0;
   while(n>0){
       rem[i]=n%2;
       n=n/2;
       i++;
   }
   printf("binary :");
   for(int j=i-1;j>=0;j--){
       printf("%d" ,rem[j]);
   }
    return 0;
}
